{
  "name": "bell-track",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 8080",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome check --write",
    "prepare": "husky",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed:prod": "npx tsx prisma/seed-prod.ts",
    "db:studio": "npx prisma studio",
    "db:backfill:user": "npx tsx scripts/backfill-user-ownership.ts",
    "ts": "yarn tsc --noEmit",
    "postinstall": "prisma generate",
    "vercel-build": "prisma migrate deploy && npm run db:seed:prod && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.6",
    "@trpc/client": "^11.7.1",
    "@trpc/next": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.552.0",
    "next": "^16.0.1",
    "next-auth": "^5.0.0-beta.30",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "type-fest": "^5.1.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json,css,scss,md}": [
      "npm run format"
    ]
  }
}
